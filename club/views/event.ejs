    <div class="container" >
        <div class="row">
            <div class="col-10">
              
              <% let i = 0; %>
              <% for(let j = 0; j < (user.length/3)+1; j++) { %>
                <% let count = 0; %>
                <div class="row ">
                  <% for( i = i; i < user.length; i++) { %>
                    <% if(count<3) { %>
                      <div class="col-4">
                      
                        <div class="card" style="border-radius: 25px;background-color: rgb(228, 221, 235);" >
                            <div class="card-body">
                              
                              <h4 style="display:inline-block;" class="card-title"><%= user[i].evtname %></h4>
                              <h6  style="float: right;" class="card-subtitle mb-2  badge bg-success"><%= user[i].fee %></h6>
                              <br>
                              <br>
                              <h6  class="card-text">Starts : <%= user[i].start %></h6>
                              <h6 style="display:inline-block" class="card-text">Ends : <%= user[i].end %></h6>
                              <a style="float: right;" href="/des/<%=i%>/<%=name%>" class="btn btn-primary">register</a>
                             
                              
                            </div>
                          </div>
                      </div>
                     
                      <%} else { %>
                        <% break; %>
                        <% } %>
                      <% count++; %>
                  <% } %>
                </div>
                <br>
                <% } %>
             </div>
          </div>   
